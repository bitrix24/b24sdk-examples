{
  "name": "b24sdk-examples/b24phpsdk-batch-cli-example",
  "description": "Example for work with bitrix24-php-sdk and local application with workflow activity",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Mesilov",
      "email": "mesilov.maxim@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "require": {
    "bitrix24/b24phpsdk": "dev-dev",
    "monolog/monolog": "^3",
    "symfony/console": "^7.3",
    "symfony/filesystem": "^7.3",
    "symfony/debug-bundle": "^7.3",
    "fig/http-message-util": "^1",
    "symfony/dependency-injection": "^7.3",
    "symfony/config": "^7.3",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2",
    "lendable/composer-license-checker": "^1",
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan": "2.*",
    "rector/rector": "2.*",
    "shipmonk/composer-dependency-analyser": "dev-master",
    "friendsofphp/php-cs-fixer": "*"
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
