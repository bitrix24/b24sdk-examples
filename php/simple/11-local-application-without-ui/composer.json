{
  "name": "b24sdk-examples/b24phpsdk-batch-cli-example",
  "description": "Example for work with bitrix24-php-sdk and local application without UI",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Mesilov",
      "email": "mesilov.maxim@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "require": {
    "bitrix24/b24phpsdk": "dev-dev",
    "monolog/monolog": "^3",
    "symfony/console": "7.2.*",
    "symfony/filesystem": "7.2.*",
    "symfony/debug-bundle": "7.2.*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2",
    "lendable/composer-license-checker": "^1",
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan": "2.*",
    "rector/rector": "2.*",
    "friendsofphp/php-cs-fixer": "*"
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
