{
  "name": "b24sdk-examples/07-webhook-sdk-sentry",
  "description": "Example for work with bitrix24-php-sdk, with webhook and sentry",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Mesilov",
      "email": "mesilov.maxim@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "require": {
    "php": "^8.3",
    "bitrix24/b24phpsdk": "1.3.*",
    "monolog/monolog": "^3",
    "symfony/dependency-injection": "^7.3",
    "symfony/config": "^7.3",
    "symfony/yaml": "^7.3",
    "sentry/sentry": "*"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2",
    "lendable/composer-license-checker": "^1",
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan": "2.*",
    "rector/rector": "2.*",
    "shipmonk/composer-dependency-analyser": "dev-master",
    "friendsofphp/php-cs-fixer": "*"
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
